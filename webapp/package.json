{
  "name": "govregistry-workspace",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start-govregistry": "ng serve govregistry-app --port 5201",
    "start-govregistry-proxy": "ng serve govregistry-app --proxy-config proxy.config.json --port 5201",
    "production-govregistry": "node ./builder.version.js --project=govregistry-app && npm run distribution-govregistry",
    "distribution-govregistry": "npm run build-govregistry && npm run clean-govregistry",
    "build-govregistry": "ng build govregistry-app  --configuration production --aot=true --base-href /govregistry-app/",
    "clean-govregistry": "rm -r ./dist/govregistry-app/assets/scss && rm -r ./dist/govregistry-app/assets/*.js",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
